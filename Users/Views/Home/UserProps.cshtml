@using Users.Models
@model AppUser
@{
    ViewBag.Title = "UserProps";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        自定义用户属性
    </div>
    <table class="table table-striped">
        <tr>
            <th>城市</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>国籍</th>
            <td>@Model.Country</td>
        </tr>
    </table>
</div>
@using (Html.BeginForm())
{
    <div class="form-group">
        <label>城市</label>
        @Html.DropDownListFor(model=>model.City,new SelectList(Enum.GetNames(typeof(Cities))))
    </div>
    <button class="btn btn-primary" type="submit">保存</button>
}

